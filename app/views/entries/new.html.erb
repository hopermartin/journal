
<%= form_for :entry, url: entries_path do |f| %>
  <p>
    <%= f.label :emoji %><br>
    <%= f.text_area :emoji %>
  </p>

  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', entries_path %>